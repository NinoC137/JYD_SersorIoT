{
    "name": "BLTGUI",
    "keywords": "bltgui",
    "description": "bltgui des",
    "version": "1.0",
    "authors": {
      "name": "lb"
    },
    "licence": "MIT",
    "frameworks": "arduino",
    "platforms": "espressif32",
    "build" : {
      "unflags": "-std=gnu++11",
      "flags": [
         "-std=c99",
         
         "-I ../../.pio/libdeps/adafruit_feather_esp32s2_tft/lvgl/src/font",
         "-I ../../.pio/libdeps/adafruit_feather_esp32s2_tft/lvgl/src/core",
         "-I ../../.pio/libdeps/adafruit_feather_esp32s2_tft/lvgl",
        "-I custom",
        "-I generated",
        "-I generated/guider_customer_fonts",
        "-I generated/guider_fonts",
        "-I generated/images"
        

        
      ],
      "srcFilter": [
        "+<*.c>",
        "+<custom/*.c>",
        "+<generated/*.c>",
        "+<generated/guider_customer_fonts/*.c>",
        "+<generated/guider_fonts/*.c>",
        "+<generated/images/*.c>"
        
      ]
    }
  }